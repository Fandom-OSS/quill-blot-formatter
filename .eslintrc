{
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended"
  ],
  "plugins": ["flowtype"],
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": 0,
    "lines-between-class-members": 0,
    "class-methods-use-this": 0,
    "no-console": 1,
    "no-mixed-operators": 0,
    "no-restricted-syntax": [
      "error",
      "ForInStatement"
    ],
    "prefer-destructuring": 0
  }
}
